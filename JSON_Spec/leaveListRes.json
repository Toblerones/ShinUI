{
	"$schema" : "http://json-schema.org/draft-04/schema#",
	"title" : "myLeaveListRes",
	"description" : "My leave request list response json",
	"type" : "object",
	"properties" : {
		"cmd":{
			"type":"string",
			"minLength":3
		},
		"responseStatus":{
			"type":"string"
		},
		"errorCode":{
			"type":"string"
		},
		"errorMessage":{
			"type":"string"
		},
		"validationErrors":{
			"type":"array",
			"items":{
				"type":"object",
				"properties":{
					"code":{
						"type":"string"
					},
					"desc":{
						"type":"string"
					}
				}
			}
		},
		"data" : {
			"description" : "Response return data",
			"type" : "object",
			"properties" : {
				"leaves": {
					"description" : "Session ID stored in cookie",
					"type" : "array",
					"items" : {
						"leave" : {
							"description" : "Leave request",
							"type" : "object",
							"properties" : {
								"leaveId" : {
									"description" : "Leave request ID",
									"type" : "string"
								},
								"status" : {
									"description" : "Leave request status",
									"type" : "number"
								},
								"leaveType" : {
									"description" : "Leave type",
									"type" : "number"
								},
								"startDate" : {
									"description" : "Leave start date",
									"type" : "string"
								},
								"endDate" : {
									"description" : "Leave end data",
									"type" : "string"
								}
							},
							"required" : ["leaveId","status","leaveType","startDate","endDate"]
						}
					}
				},
			},
			"required" : ["leaves"]
		}

	},
	"required" : ["result"]

}