{
	"$schema" : "http://json-schema.org/draft-04/schema#",
	"title" : "loginRes",
	"description" : "Login response json",
	"type" : "object",
	"properties" : {
		"cmd":{
			"type":"string",
			"minLength":3
		},
		"responseStatus":{
			"type":"string"
		},
		"errorCode":{
			"type":"string"
		},
		"errorMessage":{
			"type":"string"
		},
		"validationErrors":{
			"type":"array",
			"items":{
				"type":"object",
				"properties":{
					"code":{
						"type":"string"
					},
					"desc":{
						"type":"string"
					}
				}
			}
		},
		"data" : {
			"description" : "Response return data",
			"type" : "object",
			"properties" : {
				"sessionId": {
					"description" : "Session ID stored in cookie",
					"type" : "string"
				},
			},
			"required" : ["sessionId"]
		}

	},
	"required" : ["result"]

}