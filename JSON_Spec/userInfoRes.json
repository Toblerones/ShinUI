{
	"$schema" : "http://json-schema.org/draft-04/schema#",
	"title" : "userInfoRes",
	"description" : "Get user information response",
	"type" : "object",
	"properties" : {
		"cmd":{
			"type":"string",
			"minLength":3
		},
		"responseStatus":{
			"type":"string"
		},
		"errorCode":{
			"type":"string"
		},
		"errorMessage":{
			"type":"string"
		},
		"validationErrors":{
			"type":"array",
			"items":{
				"type":"object",
				"properties":{
					"code":{
						"type":"string"
					},
					"desc":{
						"type":"string"
					}
				}
			}
		},
		"data" : {
			"description" : "Response return data",
			"type" : "object",
			"properties" : {
				"user": {
					"description" : "User information",
					"type" : "object",
					"properties" : {
						"staffId" : {
							"description" : "Staff ID",
							"type" : "number",
						},
						"staffName" : {
							"description" : "Staff Name",
							"type" : "string",
						},
						"title" : {
							"description" : "Staff title",
							"type" : "string",
						},   
					},
					"required" : ["staffId", "staffName", "title"]
				},

			},
			"required" : ["user"]
		},
		"required" : ["result"]
	}
}